@import "/static/css/vendor.wxss";
.md-banner-img {
  width: 100%;
}
.md-abc {
  display: flex;
  flex-direction: row;
  position: relative;
  padding: 2rpx;
}
.md-abc-a,
.md-abc-bc {
  flex: 1;
}
.md-abc-r {
  padding-left: 1rpx;
}
.md-abc-img {
  width: 100%;
  float: left;
}
.md-abc-b {
  align-self: flex-start;
}
.md-abc-c {
  align-self: flex-end;
}
.md-tb {
  position: relative;
  padding: 2rpx;
}
.md-tb-t {
  margin-bottom: 1rpx;
  line-height: 0;
}
.md-tb-bc {
  display: flex;
  flex-direction: row;
}
.md-tb-b,
.md-tb-c {
  flex: 1;
}
.md-tb-img {
  width: 100%;
}
.md-tb-b {
  margin-right: 1rpx;
}
.md-tw {
  position: relative;
  display: flex;
  flex-direction: row;
}
.md-tw-a {
  max-width: 50%;
}
.md-tw-img {
  width: 100%;
}
.md-tw-b {
  flex: 1;
  padding: 5rpx;
  color: #666;
  background-color: #fff;
}
.md-tw-r {
  margin-left: 5rpx;
}
.md-prolist-a-hd {
  line-height: 36rpx;
  padding-left: 5rpx;
  font-weight: 600;
  font-size: 16rpx;
  color: #333;
  background-color: #fff;
  margin-bottom: 1rpx;
}
.md-prolist-a-more {
  float: right;
  color: #555;
  font-size: 14rpx;
  margin-right: 3rpx;
  font-weight: 400;
  cursor: pointer;
}
.md-prolist-a-more:after {
  content: "\E6A3";
  font-family: "iconfont" !important;
  display: inline-block;
  font-size: 14rpx;
  font-weight: 300;
  color: #999;
}
.md-prolist-a {
  margin-bottom: 5rpx;
}
.md-prolist-a:after {
  clear: both;
  content: ".";
  visibility: hidden;
  display: block;
  height: 0;
}
.md-prolist-a-item {
  padding: 2rpx;
  margin-bottom: 5rpx;
  box-sizing: border-box;
  width: 50%;
  float: left;
  display: block;
  cursor: pointer;
  text-decoration: none;
}
.md-prolist-a-item-img {
  margin-bottom: 5rpx;
}
.md-prolist-a-img {
  width: 100%;
}
.md-prolist-a-row {
  padding-left: 3rpx;
  padding-bottom: 3rpx;
}
.md-prolist-a-title {
  padding-left: 3rpx;
  margin-bottom: 5rpx;
  font-size: 14rpx;
  color: #444;
  white-space: nowrap;
  text-overflow: ellipsis;
  padding-left: 3rpx;
}
.md-prolist-a-price {
  color: #FF6600;
  font-size: 18rpx;
}
.md-prolist-a-pd {
  background-color: #fff;
}
.md-artlist-a:after {
  clear: both;
  content: ".";
  visibility: hidden;
  display: block;
  height: 0;
}
.md-artlist-a-hd {
  line-height: 36rpx;
  padding-left: 5rpx;
  font-weight: 600;
  font-size: 16rpx;
  color: #333;
  background-color: #fff;
  margin-bottom: 1rpx;
}
.md-artlist-a-more {
  float: right;
  color: #555;
  font-size: 14rpx;
  margin-right: 3rpx;
  cursor: pointer;
  font-weight: 400;
}
.md-artlist-a-more:after {
  content: "\E6A3";
  font-family: "iconfont" !important;
  display: inline-block;
  font-size: 14rpx;
  font-weight: 300;
  color: #999;
}
.md-artlist-a-item {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  padding: 5rpx;
  border-bottom: 1rpx solid #eee;
}
.md-artlist-a-img {
  width: 100rpx;
  margin-right: 10rpx;
}
.md-artlist-a-flex {
  flex: 1;
}
.md-artlist-a-title {
  margin-bottom: 5rpx;
  font-size: 14rpx;
  color: #444;
}
.md-artlist-a-desc {
  font-size: 12rpx;
  color: #666;
}
.md-contact-hd {
  line-height: 36rpx;
  color: #444;
  font-size: 16rpx;
  font-weight: 600;
}
.md-contact {
  background-color: #fff;
  padding: 10rpx;
  margin-bottom: 10rpx;
}
.md-contact-item {
  display: flex;
  flex-direction: row;
  margin-bottom: 10rpx;
  font-size: 16rpx;
  color: #555;
}
.md-contact-label {
  width: 80rpx;
}
.md-contact-value {
  flex: 1;
}
.md-tags-hd {
  background-color: #eee;
  border-radius: 10rpx;
  display: inline-block;
  padding: 5rpx 8rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  color: #0069CD;
}
.md-tags {
  padding: 10rpx 5rpx;
  background-color: #fff;
  margin-bottom: 5rpx;
}
.md-tags-item {
  display: inline-block;
  margin-right: 5rpx;
  text-decoration: none;
  color: #444;
  background-color: #eee;
  padding: 5rpx 8rpx;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  font-size: 14rpx;
}
.md-piclist-a-hd {
  line-height: 36rpx;
  padding-left: 5rpx;
  font-weight: 600;
  font-size: 16rpx;
  color: #333;
  background-color: #fff;
  margin-bottom: 1rpx;
}
.md-piclist-a-more {
  float: right;
  color: #555;
  font-size: 14rpx;
  margin-right: 3rpx;
  cursor: pointer;
  font-weight: 400;
}
.md-piclist-a-more:after {
  content: "\E6A3";
  font-family: "iconfont" !important;
  display: inline-block;
  font-size: 14rpx;
  font-weight: 300;
  color: #999;
}
.md-piclist-a {
  padding: 5rpx;
  background-color: #fff;
}
.md-piclist-a:after {
  clear: both;
  content: ".";
  visibility: hidden;
  display: block;
  height: 0;
}
.md-piclist-a-item {
  margin-bottom: 10rpx;
  border: 1rpx solid #efefef;
  border-radius: 10rpx;
  background-color: #fff;
  overflow: hidden;
  position: relative;
  display: block;
  cursor: pointer;
  text-decoration: none;
}
.md-piclist-a-item-img {
  position: relative;
}
.md-piclist-a-img {
  width: 100%;
}
.md-piclist-a-num {
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  color: #eee;
}
.md-piclist-a-title {
  padding-left: 10rpx;
  font-size: 14rpx;
  color: #000;
  margin-bottom: 10rpx;
}
.md-piclist-a-cm {
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  color: #666;
  font-size: 14rpx;
}
.md-nav-a {
  padding: 5rpx 0;
  background-color: #fff;
  margin: 10rpx 0;
}
.md-nav-a:after {
  clear: both;
  content: ".";
  visibility: hidden;
  height: 0;
  display: block;
}
.md-nav-a-item {
  float: left;
  width: 25%;
  margin-bottom: 5rpx;
  text-align: center;
  line-height: 1;
  padding: 5rpx;
}
.md-nav-a-img {
  width: 50rpx;
  height: 50rpx;
  border-radius: 10rpx;
  margin-bottom: 5rpx;
}
.md-nav-a-title {
  text-align: center;
  font-size: 14rpx;
  color: #444;
}
.md-float {
  position: fixed;
  right: 3rpx;
  top: 150rpx;
  background-color: #ed6d53;
  color: #fff;
  padding: 10rpx;
  z-index: 99;
  width: 32rpx;
  padding: 5rpx 8rpx;
}
.md-float ._a {
  color: #fff;
}